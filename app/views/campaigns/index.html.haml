= styles_for :campaign

= render 'list_title_bar'

.remote#create_campaign{ hidden }
.remote#campaign_import{ hidden }

%iframe#uploading{ name: "uploading", style: "width:100px; height:10px; border:5px" }

= render 'search'

.remote#options{ hidden }

.list#campaigns
  - if @campaigns.any?
    = render @campaigns
  - else
    = render "shared/empty"

#paginate= render "shared/paginate_with_per_page"
#export= render "shared/export"
